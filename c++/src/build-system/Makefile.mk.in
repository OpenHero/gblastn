# EUtils
EUTILS_LIBS = eutils egquery elink epost esearch espell esummary linkout \
              einfo uilist ehistory

# Object readers
OBJREAD_LIBS = xobjread variation creaders

# formatting code
XFORMAT_LIBS = xformat gbseq submit mlacli mla medlars pubmed
